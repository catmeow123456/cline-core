{
  "name": "cline-core",
  "version": "0.1.0",
  "description": "Minimal experimental extraction of Cline's core backend architecture for LLM tool orchestration",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "tsc",
    "dev": "tsx src/index.ts",
    "start": "node dist/index.js",
    "test": "jest",
    "clean": "rm -rf dist",
    "example": "tsx examples/basic-usage.ts",
    "example:basic": "tsx examples/basic-usage.ts --example basic",
    "example:plan": "tsx examples/basic-usage.ts --example plan",
    "example:mcp": "tsx examples/basic-usage.ts --example mcp",
    "example:all": "tsx examples/basic-usage.ts --example all",
    "example:list": "tsx examples/basic-usage.ts --list"
  },
  "keywords": [
    "ai",
    "llm",
    "agent",
    "tools",
    "mcp",
    "anthropic",
    "openai"
  ],
  "author": "Cline Core Team",
  "license": "MIT",
  "dependencies": {
    "@anthropic-ai/sdk": "^0.30.1",
    "@modelcontextprotocol/sdk": "^1.0.4",
    "@types/commander": "^2.12.5",
    "axios": "^1.7.7",
    "chalk": "^5.3.0",
    "chokidar": "^4.0.1",
    "clone-deep": "^4.0.1",
    "commander": "^14.0.0",
    "execa": "^9.4.0",
    "fast-deep-equal": "^3.1.3",
    "inquirer": "^12.0.0",
    "ora": "^8.1.0",
    "p-timeout": "^6.1.3",
    "p-wait-for": "^5.0.2",
    "puppeteer": "^23.8.0",
    "reconnecting-eventsource": "^1.6.4",
    "ulid": "^2.3.0",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@types/clone-deep": "^4.0.4",
    "@types/inquirer": "^9.0.7",
    "@types/jest": "^29.5.14",
    "@types/node": "^24.3.0",
    "jest": "^29.7.0",
    "ts-jest": "^29.2.5",
    "tsx": "^4.19.2",
    "typescript": "^5.6.3"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "packageManager": "yarn@4.9.2"
}
